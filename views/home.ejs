<%- include('header') %>
    <h2>Open Rules:
        <a href="/admin"><button type="button" class="btn btn-success btn-sm">Administrativo</button></a>
    </h2>
    <% categorias.forEach(categoria => { %>
        <div>
            <h3><%- categoria.categoria %></h3>
            <ul>
                <% categoria.vagas.forEach(vaga => { %>
                    <li><a href="vaga/<%- vaga.id %>"><%- vaga.titulo %></a></li>
                <% }) %>
            </ul>
        </div>
    <% }) %>
<%- include('footer') %>